    <!-- Ground -->
    <path
        android:pathData="M0,800 L2048,800 L2048,1536 L0,1536 Z"
        android:fillColor="#228B22" />

    <!-- Trees (using path to draw circles) -->
    <path
        android:pathData="M200,620 A80,80 0 1,1 200,780 A80,80 0 1,1 200,620 Z"
        android:fillColor="#006400" />

    <path
        android:pathData="M500,690 A60,60 0 1,1 500,810 A60,60 0 1,1 500,690 Z"
        android:fillColor="#228B22" />

    <path
        android:pathData="M800,650 A70,70 0 1,1 800,790 A70,70 0 1,1 800,650 Z"
        android:fillColor="#32CD32" />

    <path
        android:pathData="M1200,675 A65,65 0 1,1 1200,805 A65,65 0 1,1 1200,675 Z"
        android:fillColor="#006400" />

    <path
