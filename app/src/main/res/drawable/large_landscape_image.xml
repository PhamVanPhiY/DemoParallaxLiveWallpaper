        android:pathData="M280,150 A120,40 0 1,1 520,150 A120,40 0 1,1 280,150 Z"
        android:fillColor="#FFFFFF" />

    <path
        android:pathData="M900,180 A100,35 0 1,1 1100,180 A100,35 0 1,1 900,180 Z"
        android:fillColor="#F0F8FF" />

    <path
        android:pathData="M1320,120 A80,30 0 1,1 1480,120 A80,30 0 1,1 1320,120 Z"
        android:fillColor="#FFFFFF" />

    <!-- Water/Lake (using path to draw ellipse) -->
    <path
        android:pathData="M424,1200 A600,200 0 1,1 1624,1200 A600,200 0 1,1 424,1200 Z"
        android:fillColor="#4682B4" />

